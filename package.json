    "form-data": "^3.0.0",
    "fs-extra": "^7.0.1",
    "get-port": "^5.1.1",
    "json-schema-to-typescript": "^10.1.4",
    "json5": "^2.1.0",
    "node-fetch": "^2",
    "ora": "^3.4.0",
    "prettier": "^2.7.1",
    "prompts": "^2.0.4",
    "proxy-agent": "^5.0.0",
    "signal-exit": "^3.0.3",
